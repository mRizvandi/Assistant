<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <AssemblyName>AryaVtd.Orca.Assistant</AssemblyName>
    <RootNamespace>AryaVtd.Orca.Assistant</RootNamespace>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Version>1.1.0</Version>
    <AssemblyVersion>1.1.0.3</AssemblyVersion>
    <Authors>Arya Vandidad Technology Development Co.</Authors>
    <Product>Orca Framework - UI Common for Components</Product>
    <Copyright>2022 Arya Vandidad all rights reserved!</Copyright>
    <PackageProjectUrl>https://aryavandidad.com</PackageProjectUrl>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>Orca Framework is a ASP.NET Core and Blazor framework's by Arya Vandidad Technology Development Co. and this is a General Assistant for all projects on Orca.</Description>
    <PackageIcon>icon.png</PackageIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Include="icon.png" Pack="true" Visible="false" PackagePath="" />
    <None Include="icon.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
